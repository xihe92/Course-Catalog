

<h1>Listing Enrollments</h1>

<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Course Code</th>
      <th>Course Name</th>
      <th>Subject ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrollments.each do |enrollment| %>
      <tr>
        <td align="center"><%= enrollment.u_id %></td>
        <td align="center"><%= enrollment.c_code %></td>
        <td align="center"><%= enrollment.c_name %></td>
        <td align="center"><%= enrollment.s_id %></td>
        <td align="center"><%= link_to 'Show', enrollment %></td>
        <td align="center"><%= link_to 'Edit', edit_enrollment_path(enrollment) %></td>
        <td align="center"><%= link_to 'Destroy', enrollment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
